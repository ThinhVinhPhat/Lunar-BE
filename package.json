{
  "name": "be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "20.x"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*",
      "webhook/*",
      "cron/*",
      "hono/*"
    ]
  },
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "deploy": "wrangler deploy",
    "build:worker": "esbuild apps/hono/src/worker.ts --bundle --platform=node --format=esm --outfile=dist/worker.js --external:@nestjs/microservices --external:@nestjs/websockets",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "vercel-build": "npm run build",
    "start:dev": "nest start --watch",
    "start:webhook": "nest start @apps/webhook",
    "start:main": "nest start @apps/main",
    "start:cron": "nest start @apps/cron",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/main/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:up": "npm run build && npm run typeorm migration:run -- -d ./libs/database/src/typeorm.cli.ts",
    "migration:generate": "npm run typeorm -- -d ./libs/database/src/typeorm.cli.ts migration:generate ./migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "migration:down": "npm run typeorm -- -d ./libs/database/src/typeorm.ts migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/lib-storage": "^3.726.1",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@google/generative-ai": "^0.24.1",
    "@neondatabase/serverless": "^1.0.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.18",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.18",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.0.2",
    "api-query-params": "5.4.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cache-manager-ioredis": "^2.1.0",
    "chanfana": "^2.8.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "1.11.13",
    "handlebars": "^4.7.8",
    "hono": "^4.9.1",
    "ioredis": "^5.6.1",
    "mongoose": "^8.6.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "passport": "0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1",
    "stripe": "^17.5.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/decorator(|/.*)$": "<rootDir>/libs/decorator/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/helper(|/.*)$": "<rootDir>/libs/helper/src/$1",
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@app/type(|/.*)$": "<rootDir>/libs/type/src/$1",
      "^@app/stripe(|/.*)$": "<rootDir>/libs/stripe/src/$1",
      "^@app/entity(|/.*)$": "<rootDir>/libs/entity/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/constant(|/.*)$": "<rootDir>/libs/constant/src/$1",
      "^@app/mail(|/.*)$": "<rootDir>/libs/mail/src/$1",
      "^@app/middleware(|/.*)$": "<rootDir>/libs/middleware/src/$1",
      "^@app/interceptors(|/.*)$": "<rootDir>/libs/interceptors/src/$1",
      "^@app/filter(|/.*)$": "<rootDir>/libs/filter/src/$1",
      "^@app/pipe(|/.*)$": "<rootDir>/libs/pipe/src/$1",
      "^@app/gateway(|/.*)$": "<rootDir>/libs/gateway/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
