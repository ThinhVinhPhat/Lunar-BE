{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "mail/template/**/*"
    ],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "@apps/hono": {
      "type": "application",
      "root": "apps/hono",
      "entryFile": "main",
      "sourceRoot": "apps/hono/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hono/tsconfig.app.json"
      }
    },
    "@apps/main-service": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
