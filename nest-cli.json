{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "mail/template/**/*"
    ],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "@apps/main-service": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "@apps/webhook": {
      "type": "application",
      "root": "apps/webhook",
      "entryFile": "main",
      "sourceRoot": "apps/webhook/src",
      "compilerOptions": {
        "tsConfigPath": "apps/webhook/tsconfig.app.json"
      }
    },
    "@app/decorator": {
      "type": "library",
      "root": "libs/decorator",
      "entryFile": "index",
      "sourceRoot": "libs/decorator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorator/tsconfig.lib.json"
      }
    },
    "@app/database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "@app/helper": {
      "type": "library",
      "root": "libs/helper",
      "entryFile": "index",
      "sourceRoot": "libs/helper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helper/tsconfig.lib.json"
      }
    },
    "@app/shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "@app/types": {
      "type": "library",
      "root": "libs/types",
      "entryFile": "index",
      "sourceRoot": "libs/types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/types/tsconfig.lib.json"
      }
    },
    "@app/stripe": {
      "type": "library",
      "root": "libs/stripe",
      "entryFile": "index",
      "sourceRoot": "libs/stripe/src",
      "compilerOptions": {
        "tsConfigPath": "libs/stripe/tsconfig.lib.json"
      }
    },
    "@app/entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "@app/config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "@app/constant": {
      "type": "library",
      "root": "libs/constant",
      "entryFile": "index",
      "sourceRoot": "libs/constant/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constant/tsconfig.lib.json"
      }
    },
    "@app/mail": {
      "type": "library",
      "root": "libs/mail",
      "entryFile": "index",
      "sourceRoot": "libs/mail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mail/tsconfig.lib.json"
      }
    }
  }
}